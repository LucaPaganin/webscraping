{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: INPA",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/inpa/inpa.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: movie_downloader",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/moviedownloader/moviedownloader.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: ebay scraper",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ebay/scrape_main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Streamlit: python ebay scraper",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/Scripts/streamlit.exe",
            "args": [
                "run",
                "${workspaceFolder}/ebay/streamlit_scraper.py"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: ryanair flight monitor",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ryanair/monitor_main.py",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            // "env": {
            //     "PYTHONPATH": "${workspaceFolder}/ryanair"
            // }
        },
        {
            "name": "[scrapy-spider]: immobit",
            "type": "python",
            "request": "launch",
            "module": "scrapy",
            "args": [
                "runspider",
                "immob/spiders/immobit.py"
            ],
            "cwd": "${workspaceFolder}/immob",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            }
        },
    ]
}