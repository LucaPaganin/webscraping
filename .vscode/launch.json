{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "[agent]: browser-user agent",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/browser_use/main.py",
            "console": "integratedTerminal",
            "presentation": {
                "group": "agent",
                "order": 1
            }
        },
        {
            "name": "Python: movie_downloader",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/moviedownloader/moviedownloader.py",
            "console": "integratedTerminal",
            "presentation": {
                "group": "Movie Downloader",
                "order": 1
            }
        },
        {
            "name": "Python: ebay scraper",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ebay/scrape_main.py",
            "console": "integratedTerminal",
            "presentation": {
                "group": "Ebay Scraper",
                "order": 1
            }
        },
        {
            "name": "Streamlit: ebay/vinted scraper",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/Scripts/streamlit.exe",
            "args": [
                "run",
                "${workspaceFolder}/ebay/streamlit_scraper.py"
            ],
            "console": "integratedTerminal",
            "presentation": {
                "group": "Streamlit",
                "order": 1
            }
        },
        {
            "name": "Python: ryanair flight monitor",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ryanair/monitor_main.py",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            // "env": {
            //     "PYTHONPATH": "${workspaceFolder}/ryanair"
            // }
            "presentation": {
                "group": "Ryanair",
                "order": 1
            }
        },
        {
            "name": "[scrapy-spider]: immobit",
            "type": "debugpy",
            "request": "launch",
            "module": "scrapy",
            "args": [
                "crawl",
                "immobit",
                "-L", "DEBUG",
                "--set=CLOSESPIDER_PAGECOUNT=3",
                "--set=DOWNLOAD_DELAY=2",
                "--set=COOKIES_DEBUG=True"
            ],
            "cwd": "${workspaceFolder}/immob",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "presentation": {
                "group": "Immobit",
                "order": 1
            }
        },
        {
            "name": "[scrapy]: immobit_selenium",
            "type": "debugpy",
            "request": "launch",
            "module": "scrapy",
            "args": [
                "crawl",
                "immobit_selenium",
                "-L", "DEBUG",
                "--set=CLOSESPIDER_PAGECOUNT=3",
                "--set=DOWNLOAD_DELAY=2",
                "--set=COOKIES_DEBUG=True"
            ],
            "cwd": "${workspaceFolder}/immob",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "presentation": {
                "group": "Immobit",
                "order": 2
            }
        },
        {
            "name": "[scrapy-spider]: immobit (single URL)",
            "type": "debugpy",
            "request": "launch",
            "module": "scrapy",
            "args": [
                "crawl",
                "immobit",
                "-L", "DEBUG",
                "-a", "start_url=https://www.immobiliare.it/annunci/120065826/",
                "--set=COOKIES_DEBUG=True",
                "--set=ROBOTSTXT_OBEY=False",
                "--set=CONCURRENT_REQUESTS=1",
                "--set=DOWNLOAD_DELAY=5"
            ],
            "cwd": "${workspaceFolder}/immob",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "presentation": {
                "group": "Immobit",
                "order": 3
            }
        },
        {
            "name": "[scrapy-spider]: immobit (with proxy)",
            "type": "debugpy",
            "request": "launch",
            "module": "scrapy",
            "args": [
                "crawl",
                "immobit",
                "-L", "DEBUG",
                "-a", "start_url=https://www.immobiliare.it/annunci/120065826/",
                "--set=COOKIES_DEBUG=True",
                "--set=ROBOTSTXT_OBEY=False",
                "--set=CONCURRENT_REQUESTS=1",
                "--set=DOWNLOAD_DELAY=10",
                "--set=PROXY_LIST=${workspaceFolder}/immob/proxies.txt",
                "--set=PROXY_MODE=0"
            ],
            "cwd": "${workspaceFolder}/immob",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "presentation": {
                "group": "Immobit",
                "order": 4
            }
        },
        {
            "name": "immobit debug script",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/immob/Untitled.py",
            "args": [],
            "cwd": "${workspaceFolder}/immob",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "presentation": {
                "group": "Immobit",
                "order": 5
            }
        },
        {
            "name": "[scrapy-shell]: immobiliare.it",
            "type": "debugpy",
            "request": "launch",
            "module": "scrapy",
            "args": [
                "shell",
                "https://www.immobiliare.it/annunci/120065826/"
            ],
            "cwd": "${workspaceFolder}/immob",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "presentation": {
                "group": "Immobit",
                "order": 6
            }
        }
    ]
}
